/ {
	model = "GameConsole U8";
	compatible = "gameconsole,u8", "rockchip,rk3326";
	
	aliases {
		mmc0 = &emmc;
		mmc1 = &sdmmc;
		mmc2 = &sdio;
	};
	
	chosen {
		systemd.debug_shell = "ttyS0";
	};
	
	backlight: backlight {
		compatible = "pwm-backlight";
		pwms = <&pwm1 0 25000 0>;
		brightness-levels = <
		0   1   2   3   4   5   6   7
