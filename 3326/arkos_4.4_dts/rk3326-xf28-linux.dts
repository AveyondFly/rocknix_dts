&pinctrl {
	pmic {
		pmic_int: pmic_int {
			rockchip,pins =
			<0 RK_PB2 RK_FUNC_GPIO &pcfg_pull_up>;
		};
		
		soc_slppin_gpio: soc_slppin_gpio {
			rockchip,pins =
			<0 RK_PA4 RK_FUNC_GPIO &pcfg_output_low>;
		};
		
		soc_slppin_slp: soc_slppin_slp {
			rockchip,pins =
			<0 RK_PA4 RK_FUNC_1 &pcfg_pull_none>;
		};
		
		soc_slppin_rst: soc_slppin_rst {
			rockchip,pins =
			<0 RK_PA4 RK_FUNC_2 &pcfg_pull_none>;
		};
	};
	
	sdio {
		
		sdio_clk: sdio-clk {
			rockchip,pins = <1 RK_PC5 RK_FUNC_1 &pcfg_pull_none>;
		};
		
		sdio_cmd: sdio-cmd {
			rockchip,pins = <1 RK_PC4 RK_FUNC_1 &pcfg_pull_up>;
		};
		
		sdio_bus4: sdio-bus4 {
			rockchip,pins =
			<1 RK_PC6 RK_FUNC_1 &pcfg_pull_up>,
			<1 RK_PC7 RK_FUNC_1 &pcfg_pull_up>,
			<1 RK_PD0 RK_FUNC_1 &pcfg_pull_up>,
			<1 RK_PD1 RK_FUNC_1 &pcfg_pull_up>;
		};
	};
	
	vcc5-usb {
		vcc5_usb: vcc5-usb {
			rockchip,pins = <3 RK_PA4 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};
	
	sdio-pwrseq {
		wifi_enable_h: wifi-enable-h {
			rockchip,pins = <0 RK_PA2 RK_FUNC_GPIO &pcfg_output_high>;
		};

		wifi_host_wake_l: wifi-host-wake-l {
			rockchip,pins = <0 RK_PA1 RK_FUNC_GPIO &pcfg_pull_down>;
		};
	};
	
	btns {
		btn_pins: btn-pins {
			rockchip,pins =
			<3 RK_PC6 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PC7 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PC4 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PC5 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PC0 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PC1 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PC3 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PC2 RK_FUNC_GPIO &pcfg_pull_up>,
			<2 RK_PB6 RK_FUNC_GPIO &pcfg_pull_up>,
			<2 RK_PB5 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PD2 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PB5 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PB3 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PD0 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PA0 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PA6 RK_FUNC_GPIO &pcfg_pull_up>,
			<3 RK_PD1 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};
	
	headphone {
		hp_det: hp-det {
			rockchip,pins = <2 RK_PC6 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};
};

