&display_subsystem {
	status = "okay";
};

&dsi {
	status = "okay";

	panel@0 {
		compatible = "elida,kd35t133", "simple-panel-dsi";
		reg = <0>;
		/* pwm1 backlight */
		backlight = <&backlight>;
		/* RK817 LDO7 */
		backlight-supply = <&vcc_backlight>;
		/* RK817 LDO8 */

		power-supply = <&vcc18_lcd0>;
		/* LCD reset gpio GPIO3.B7 */
		reset-gpios = <&gpio3 RK_PB7 GPIO_ACTIVE_LOW>;
		/* power-supply-gpio GPIO0.B5  */
		enable-gpios = <&gpio0 RK_PB5 GPIO_ACTIVE_HIGH>;
		prepare-delay-ms = <20>;
		reset-delay-ms = <20>;
		init-delay-ms = <20>;
		enable-delay-ms = <120>;
		disable-delay-ms = <20>;
		unprepare-delay-ms = <20>;
		width-mm = <153>;
		height-mm = <85>;
		dsi,flags  = <(MIPI_DSI_MODE_VIDEO |
					MIPI_DSI_MODE_VIDEO_BURST |
					MIPI_DSI_MODE_LPM |
					MIPI_DSI_MODE_EOT_PACKET)>;
		dsi,format = <MIPI_DSI_FMT_RGB888>;
		dsi,lanes = <4>;
		panel-init-sequence = [
			/* 基本页/寄存器 */
			39 00 04 B9 F1 12 87
			39 00 04 B2 78 04 70
			39 00 0B B3 10 10 FC FC 03 FF 00 00 00 00
			15 00 02 B4 80
			15 00 03 B5 06 06
			15 00 03 B6 A9 A9
			39 00 05 B8 26 22 F0 13

			/* 时序/电源相关 */
			39 00 1C BA 33 81 05 F9 0E 0E 20 00 00 00 00 00 00 00 44 25 00 91 0A 00 00 01 4F 01 00 00 37
			39 00 02 BC 47
			39 00 06 BF 02 10 00 80 04
			39 00 0A C0 73 73 50 50 00 00 12 73 00
			39 00 12 C1 25 C0 32 32 99 E4 77 77 CC CC FF FF 11 11 00 00 32
			39 00 0D C7 10 00 0A 00 00 00 00 00 ED C5 00 A5
			39 00 05 C8 10 40 1E 03
			39 00 02 CC 0B

			/* Gamma/色度 */
			39 00 23 E0 00 0A 0F 29 3C 3F 41 38 08 0C 0F 12 14 12 13 12 19
						00 0A 0F 29 3C 3F 41 38 08 0C 0F 12 14 12 13 12 19
			39 00 08 E1 11 11 91 00 00 00 00
			39 00 0F E3 07 07 0B 0B 0B 0B 00 00 00 00 FF 04 C0 10

			/* 接口/时钟特性块 */
			39 00 40 E9 C8 10 0A 00 00 F0 81 12 31 23 4F 86 F0 81 47 08 00 00 10 00 00 00 00 00 10 00 00 00
						88 88 08 F8 F4 46 60 02 28 88 88 88 88 18 F8 F5 57 71 13 38 88 88 00 00 00 01
						00 00 00 00 00 00 00 00 00
			39 00 3E EA 00 1A 00 00 00 00 00 00 00 00 00 00
						88 F8 18 F8 83 31 17 75 58 88 88 88 F8 08 F8 82 20 06 64 48 88 88 23 00 00 02
						FE 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00

			/* 结束/开屏 */
			39 00 04 EF FF FF 01
			05 FA 01 11  /* Sleep Out,  delay 250ms */
			05 32 01 29  /* Display On, delay 50ms  */
		];

		panel-exit-sequence = [
			05 00 01 28  /* Display Off */
			05 00 01 10  /* Sleep In    */
		];


		display-timings {
			native-mode = <&timing0>;

			timing0: 59_98Hz {
				clock-frequency = <32000000>;
				hactive = <640>;
				vactive = <480>;
				hfront-porch = <150>;
				hsync-len = <120>;
				hback-porch = <120>;
				vfront-porch = <20>;
				vsync-len = <6>;
				vback-porch = <12>;
				hsync-active = <0>;
				vsync-active = <0>;
				de-active = <0>;
				pixelclk-active = <0>;
			};
		};
	};
};

&dsi_in_vopb {
	status = "okay";
};

