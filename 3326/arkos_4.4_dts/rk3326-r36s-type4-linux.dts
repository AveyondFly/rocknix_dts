&emmc {
    compatible = "rockchip,px30-dw-mshc", "rockchip,rk3288-dw-mshc";
    status = "okay";
    bus-width = <8>;
    non-removable;
    disable-wp;
    no-sd;
    no-sdio;
    supports-emmc;
    cap-mmc-highspeed;
    clock-names = "biu", "ciu";
    clocks = <&cru HCLK_EMMC>, <&cru SCLK_EMMC>;
    vmmc-supply  = <&vcc_3v0>;
    vqmmc-supply = <&vcc1v8_soc>;
    pinctrl-names = "default";
    pinctrl-0 = <
        &emmc_clk
        &emmc_cmd
        &emmc_bus8
        &emmc_pwren
        &emmc_rstnout
    >;
    fifo-depth = <256>;
    max-frequency = <150000000>;
    num-slots = <1>;
};

